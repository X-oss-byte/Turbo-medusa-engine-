{
  "name": "@medusajs/medusa-js",
  "version": "1.3.4",
  "description": "Client for Medusa Commerce Rest API",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.js",
  "types": "dist/cjs/index.d.ts",
  "files": [
    "dist",
    "!**/*.d.ts.map"
  ],
  "scripts": {
    "build": "concurrently \"npm run build:esm\" \"npm run build:cjs\"",
    "build:esm": "tsc --project ./tsconfig.json --module ES2015 --outDir ./dist/esm",
    "build:cjs": "tsc --project ./tsconfig.json --module CommonJS --outDir ./dist/cjs",
    "prepare": "cross-env NODE_ENV=production yarn run build",
    "test": "jest --passWithNoTests"
  },
  "author": "Oliver Juhl",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.24.0",
    "form-data": "^4.0.0",
    "qs": "^6.10.3",
    "retry-axios": "^2.6.0"
  },
  "peerDependencies": {
    "@medusajs/medusa": "^1.7.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/medusajs/medusa",
    "directory": "packages/medusa-js"
  },
  "bugs": {
    "url": "https://github.com/medusajs/medusa/issues"
  },
  "devDependencies": {
    "@medusajs/medusa": "^1.7.0",
    "@types/jest": "^26.0.19",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "jest": "^26.6.3",
    "typescript": "^4.1.3"
  },
  "gitHead": "cd1f5afa5aa8c0b15ea957008ee19f1d695cbd2e",
  "homepage": "https://docs.medusajs.com/js-client/overview"
}
